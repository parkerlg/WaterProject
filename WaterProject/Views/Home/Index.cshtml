@{
    ViewData["Title"] = "The Gift of Water";
}

@model IQueryable<Project>

<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
</div>

<div>
    @foreach (var x in Model)
    {
       <div>
           <h4>@x.Program</h4>
           <h6>@x.Type</h6>
           <h6>@x.Impact</h6>
       </div>
    }
</div>